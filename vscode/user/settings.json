{
    "workbench.colorTheme": "Dark (Molokai)",
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.tabSize": 2,
    "terminal.integrated.shell.linux": "/root/terminal.sh",
    "editor.fontFamily": "'DejaVu Sans Mono for Powerline', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "workbench.editor.restoreViewState": true,
    "window.restoreWindows": "all",
    "files.autoSave": "off",
    "window.zoomLevel": 0,
    "files.eol": "\n",
    "terminal.integrated.fontSize": 13,
    "editor.fontSize": 13,
    "[go]": {},
    "go.autocompleteUnimportedPackages": true,
    "ruby.format": "rubocop",
    "ruby.codeCompletion": "rcodetools",
    "ruby.useLanguageServer": true,
    "go.useLanguageServer": false,
    "go.formatTool": "goimports",
    "cursor-ruler.width": "10px",
    "cursor-ruler.color": "#073642",
    "breadcrumbs.enabled": true
}

